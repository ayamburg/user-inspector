{
  "short_name": "User Checker",
  "name": "User Checker",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
},
  "content_scripts": [
    {
      "matches": ["https://*.reddit.com/*"],
      "run_at": "document_end",
      "css": ["/static/css/Hover.css"],
      "js": ["/static/js/Hover.js"]
    }
  ]
}
